{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "amd": true,
    "commonjs": true,
    "jasmine": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-var": 0,
    "func-names": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "no-console": 0,
    "no-param-reassign": ["error", { "props": false }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prefer-stateless-function": 0,
    "arrow-parens": 0,
    "consistent-return": 0,
    "arrow-body-style": 0,
    "react/self-closing-comp": 0,
    "react/forbid-prop-types": 0,
    "react/prefer-stateless-function": 0
  },
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}